    <error-code>404</error-code>
    <location>/error/error_404.jsp</location>
  </error-page>
  <context-param>
    <description> IMG_SRC </description>
    <param-name>IMG_SRC</param-name>
    <param-value>http://ci2017whiteday.dongyangmirae.kr/JSP/IMG_SRC</param-value>
  </context-param>
</web-app>